{
  "name": "sensorium",
  "version": "0.4.8",
  "description": "makeblock mainboards protocol api",
  "main": "browser/sensorium.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "keywords": [
    "makeblock",
    "robot",
    "IOT"
  ],
  "scripts": {
    "build": "webpack",
    "dev": "webpack --config webpack.dev.js",
    "test": "mocha --compilers js:babel-core/register",
    "test-parse": "mocha --compilers js:babel-core/register ./test/core-test/parse.test.js",
    "coverage-i": " istanbul cover node_modules/mocha/bin/_mocha ./test/interface/interface_test.js",
    "coverage-p": " istanbul cover node_modules/mocha/bin/_mocha ./test/protocol-test",
    "jsdoc": "npm run cpdoc && node_modules/.bin/jsdoc -c jsdoc_conf.json",
    "cpdoc": "node ./docs_editor/script.js && cp ./browser/sensorium.js ./docs/statics/",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Makeblock",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "blessed": "^0.1.81",
    "chai": "^3.5.0",
    "color-convert": "^1.9.1",
    "core-js": "^2.5.3",
    "detect-indent": "^5.0.0",
    "domhandler": "^2.4.1",
    "ejs": "^2.5.7",
    "eslint": "^4.4.1",
    "events": "^1.1.1",
    "jsdoc": "^3.5.4",
    "lodash.merge": "^4.6.1",
    "log": "^1.4.0",
    "mocha": "^3.5.0",
    "mocha-junit-reporter": "^1.13.0",
    "serialport": "^4.0.7",
    "ts-loader": "^2.3.4",
    "tui-jsdoc-template": "^1.2.2",
    "uglify-js": "^3.3.12",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "xml2js": "^0.4.17"
  },
  "dependencies": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    },
    "globals": {
      "Promise": false,
      "ArrayBuffer": false,
      "Uint8Array": false,
      "Int8Array": false
    }
  },
  "eslintIgnore": [
    "./node_modules/*",
    "./test/*",
    "./jsdoc/*"
  ]
}
